@model Register

@{
    ViewData["Title"] = "Become a donor";

}



<div class="main-wrapper">
    <section class="inner-hero bg_img overlay--one" style="background-image: url('assets/images/frontend/breadcrumb/61023f89990b81627537289.jpg');">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6 text-center">
                    <h2 class="page-title text-white">Become a Donor</h2>
                    <ul class="page-breadcrumb justify-content-center">
                        <li><a href="~">Home</a></li>
                        <li>Become a Donor</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <section class="pt-100 pb-100 position-relative z-index section--bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <form method="POST" asp-controller="account" asp-action="register" class="contact-form bg-white p-sm-5 p-3 rounded-3 position-relative" enctype="multipart/form-data">
                        <input type="hidden" name="role" value="Donor">                    
                        <h5 class="mb-3">Personal Information</h5>
                        <div class="row mb-4">
                            <div class="form-group col-lg-6">
                                <label for="name">Name <sup class="text--danger">*</sup></label>
                                <input type="text" asp-for="FullName" id="name" value="" placeholder="Full name" class="form--control" maxlength="80" required="">
                            </div>
                            <div class="form-group col-lg-6">
                                <label for="email">Email <sup class="text--danger">*</sup></label>
                                <input type="email" asp-for="Email" id="email" value="" placeholder="Enter Email" class="form--control" maxlength="60" required="">
                            </div>
                            <div class="form-group col-lg-6">
                                <label for="phone">Phone <sup class="text--danger">*</sup></label>
                                <input type="text" asp-for="PhoneNO" id="phone" value="" placeholder="Enter Phone" class="form--control" maxlength="40" required="">
                            </div>
                            <div class="form-group col-lg-6">
                                <label for="city">City <sup class="text--danger">*</sup></label>
                                <select asp-for="City" id="city" class="select" asp-items="@(new SelectList(ViewBag.location,"LocationId","Name"))" required>
                                    <option value="" selected="" disabled="">Select City</option>
                                </select>
                            </div>
                          @*  <div class="form-group col-lg-6">
                                <label for="location">Location <sup class="text--danger">*</sup></label>
                                <select name="location" id="location" class="select" required="">
                                    <option value="" selected="" disabled="">Select One</option>
                                </select>
                            </div>*@
                            <div class="form-group col-lg-6">
                                <label for="address">Address <sup class="text--danger">*</sup></label>
                                <input type="text"asp-for="Address" id="address" value="" placeholder="Enter Address" class="form--control" maxlength="255" required="">
                            </div>
                            <div class="form-group col-lg-6">
                                <label for="address">Password <sup class="text--danger">*</sup></label>
                                <input type="password" asp-for="Password" id="address" value="" placeholder="Enter Password" class="form--control" maxlength="255" required="">
                            </div>

                        </div>


